#include <string>
#include <vector>
#include <algorithm>
#include <iostream>
using namespace std;

string solution(vector<string> seoul) {
    string answer = "";
    vector<string>::iterator it=find(seoul.begin(),seoul.end(),"Kim");
    int i=it-seoul.begin();
    answer="김서방은 "+to_string(i)+"에 있다";
    return answer;
}
